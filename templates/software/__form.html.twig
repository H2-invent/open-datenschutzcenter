{% trans_default_domain 'software' %}
{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
<fieldset>
    {{ form_row(form.nummer) }}
    {{ form_row(form.reference) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.status) }}
    {{ form_row(form.build) }}
    {{ form_row(form.licenseType) }}
    {{ form_row(form.license) }}
    {{ form_row(form.location) }}
    {{ form_row(form.archiving) }}
    {{ form_row(form.recovery) }}
    {{ form_row(form.permissions) }}
    {{ form_row(form.licenseExpiration) }}
    {{ form_row(form.purchase) }}
</fieldset>

<fieldset>
    <legend>{% trans from 'general' %}processings{% endtrans %}</legend>
    {{ form_row(form.vvts) }}
</fieldset>

<fieldset>
    <legend>{% trans %}dataSharing{% endtrans %}</legend>
    {{ form_row(form.datenweitergabe) }}
</fieldset>

{{ form_row(form.save, {row_attr: {class: 'form-buttons'}}) }}
{{ form_rest(form) }}
{{ form_end(form) }}
