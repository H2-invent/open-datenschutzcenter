{% trans_default_domain 'dashboard' %}
<!--Card-->
<div>
    <!-- Card header -->
    <div>
        {% trans %}criticalProcesses{% endtrans %}
    </div>
    <!--Card content-->
    <div>
        <div>
            <div>
                <h5>{% trans %}auditQuestions{% endtrans %}</h5>
                <ul>
                    {% for a in kAudit %}
                        <li>
                            <a href="{{ path('audit_tom_edit', {'tom':a.id}) }}"><img style="width: 17px"
                                                                                      src="{{ asset('icons/pruefung.svg') }}"> {{ a.createdAt|date('d.m.Y H:i') }}
                                - {{ a.frage }}</a></li>
                    {% endfor %}
                </ul>
            </div>
            <div>
                <h5>{% trans %}vvts{% endtrans %}</h5>
                <ul>
                    {% for v in kVvt %}
                        <li>
                            <a href="{{ path('vvt_edit', {'id':v.id}) }}"><img style="width: 17px"
                                                                               src="{{ asset('icons/prozess.svg') }}"> {{ v.createdAt|date('d.m.Y H:i') }}
                                - {{ v.name }}</a></li>
                    {% endfor %}
                </ul>
            </div>
            <div>
                <h5>{% trans %}openDsfa{% endtrans %}</h5>
                <ul>
                    {% for d in openDsfa %}
                        <li>
                            <a href="{{ path('dsfa_edit', {'dsfa':d.id}) }}">{{ d.createdAt|date('d.m.Y H:i') }}
                                - {{ d.vvt.name }}
                                ({% trans %}{% if not d.dsb %}dsbMissing{% else %}resultMissing{% endif %}{% endtrans %}
                                )</a></li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>

</div>
<!--/.Card-->