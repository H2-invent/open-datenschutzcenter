{% trans_default_domain 'vorfall' %}
{{ form_start(form) }}
<fieldset>
    <legend>{% trans %}incident.assign{% endtrans %}</legend>
    {{ form_row(form.fakten) }}
    {{ form_row(form.datum) }}
    {{ form_row(form.auswirkung) }}
    {{ form_row(form.massnahmen) }}
    {{ form_row(form.personen) }}
    {{ form_row(form.daten) }}
</fieldset>
<fieldset>
    <legend>{% trans %}incident.report{% endtrans %}</legend>
    {{ form_row(form.gemeldet) }}
    {{ form_row(form.auftraggeberGemeldet) }}
    {{ form_row(form.betroffeneGemeldet) }}
</fieldset>
{{ form_row(form.save, { row_attr: {class: 'form-buttons'} }) }}
{{ form_rest(form) }}
{{ form_end(form) }}